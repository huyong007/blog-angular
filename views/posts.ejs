<h1>
    <%= title %>
</h1>
<p>测试分支切换</p>
<div>
    <button onclick="toIndex()"><a href='./'>click me to index</a></button>
</div>
<br/>
<div class="pic_container">
    <div class="pic_container_item" v-for="item in picList">
        <img :src="item.picture.large" />
        <div>phone:{{item.phone}}</div>
    </div>
</div>


<script>
    function toIndex() {
        console.log('click test');
    }
    var vm = new Vue({
        el: '#app',
        data: {
            picList: [],
        },
        methods: {
            fetchData() {
                axios.get('https://api.randomuser.me/',
                    {
                        params: {
                            results: 200,
                            pi: 1,
                            ps: 20,
                            _allow_anonymous: true
                        }
                    })
                    .then(function (res) {
                        vm.picList = res.data.results;
                    });
            }
        }
    })
    vm.fetchData();
</script>